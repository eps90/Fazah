<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="Eps\Fazah\Core\Model\Project" table="projects">
        <id name="projectId" column="id" type="project_id" />

        <field name="name" type="text" nullable="false" />
        <field name="enabled" type="boolean" nullable="false" />
        <field name="createdAt" type="carbondatetime" nullable="false" column="created_at" />
        <field name="updatedAt" type="carbondatetime" nullable="true" column="updated_at" />
    </entity>
</doctrine-mapping>
